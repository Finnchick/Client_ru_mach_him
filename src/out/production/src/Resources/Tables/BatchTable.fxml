<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="371.0" prefWidth="482.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.Tables.BatchController">
   <children>
      <TableView fx:id="tableview" onMouseClicked="#TableViewClicked" prefHeight="371.0" prefWidth="336.0">
         <columns>
            <TableColumn fx:id="columnId" prefWidth="37.0" text="ID" />
            <TableColumn fx:id="columnDate" prefWidth="62.0" text="Дата" />
            <TableColumn fx:id="columnWorker" prefWidth="131.0" text="Сотрудник" />
            <TableColumn fx:id="columnUser" prefWidth="105.0" text="Пользователь" />
         </columns>
      </TableView>
      <Button fx:id="buttonAdd" layoutX="345.0" layoutY="14.0" mnemonicParsing="false" onAction="#Add_Pressed" prefHeight="61.0" prefWidth="124.0" text="Добавить" />
      <Button fx:id="buttonDelete" disable="true" layoutX="345.0" layoutY="158.0" mnemonicParsing="false" onAction="#Delete_Pressed" prefHeight="61.0" prefWidth="124.0" text="Удалить" />
      <Button fx:id="buttonEdit" disable="true" layoutX="345.0" layoutY="229.0" mnemonicParsing="false" onAction="#Edit_Pressed" prefHeight="61.0" prefWidth="124.0" text="Редактировать" />
      <Button fx:id="buttonBack" layoutX="345.0" layoutY="86.0" mnemonicParsing="false" onAction="#Back_Pressed" prefHeight="61.0" prefWidth="124.0" text="Назад" />
   </children>
</AnchorPane>
