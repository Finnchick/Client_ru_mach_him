<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="379.0" prefWidth="527.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.Tables.UserController">
   <children>
      <TableView fx:id="tableview" layoutY="-1.0" onMouseClicked="#TableViewClicked" prefHeight="379.0" prefWidth="379.0">
         <columns>
            <TableColumn fx:id="columnId" prefWidth="37.0" text="ID" />
            <TableColumn fx:id="columnLogin" prefWidth="115.0" text="Логин" />
            <TableColumn fx:id="columnPassword" prefWidth="110.0" text="Пароль" />
            <TableColumn fx:id="columnRole" prefWidth="120.0" text="Роль" />
         </columns>
      </TableView>
      <Button fx:id="buttonAdd" layoutX="391.0" layoutY="14.0" mnemonicParsing="false" onAction="#Add_Pressed" prefHeight="61.0" prefWidth="124.0" text="Добавить" />
      <Button fx:id="buttonDelete" disable="true" layoutX="391.0" layoutY="159.0" mnemonicParsing="false" onAction="#Delete_Pressed" prefHeight="61.0" prefWidth="124.0" text="Удалить" />
      <Button fx:id="buttonEdit" disable="true" layoutX="391.0" layoutY="88.0" mnemonicParsing="false" onAction="#Edit_Pressed" prefHeight="61.0" prefWidth="124.0" text="Редактировать" />
      <Button fx:id="buttonBack" layoutX="391.0" layoutY="229.0" mnemonicParsing="false" onAction="#Back_Pressed" prefHeight="61.0" prefWidth="124.0" text="Назад" />
   </children>
</AnchorPane>
