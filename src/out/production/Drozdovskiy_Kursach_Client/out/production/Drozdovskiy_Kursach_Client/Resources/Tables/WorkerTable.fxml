<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="459.0" prefWidth="549.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.Tables.WorkerController">
   <children>
      <TableView fx:id="tableview" onMouseClicked="#TableViewClicked" prefHeight="459.0" prefWidth="406.0">
         <columns>
            <TableColumn fx:id="columnId" prefWidth="37.0" text="ID" />
            <TableColumn fx:id="columnName" prefWidth="127.0" text="ФИО" />
            <TableColumn fx:id="columnPosition" prefWidth="136.0" text="Должность" />
            <TableColumn fx:id="columnDepartment" prefWidth="113.0" text="Отдел" />
         </columns>
      </TableView>
      <Button fx:id="buttonAdd" layoutX="414.0" layoutY="14.0" mnemonicParsing="false" onAction="#Add_Pressed" prefHeight="61.0" prefWidth="124.0" text="Добавить" />
      <Button fx:id="buttonDelete" disable="true" layoutX="414.0" layoutY="156.0" mnemonicParsing="false" onAction="#Delete_Pressed" prefHeight="61.0" prefWidth="124.0" text="Удалить" />
      <Button fx:id="buttonEdit" disable="true" layoutX="414.0" layoutY="85.0" mnemonicParsing="false" onAction="#Edit_Pressed" prefHeight="61.0" prefWidth="124.0" text="Редактировать" />
      <Button fx:id="buttonBack" layoutX="414.0" layoutY="230.0" mnemonicParsing="false" onAction="#Back_Pressed" prefHeight="61.0" prefWidth="124.0" text="Назад" />
   </children>
</AnchorPane>
