<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="386.0" prefWidth="761.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.Tables.ProduceController">
   <children>
      <TableView fx:id="tableview" onMouseClicked="#TableViewClicked" prefHeight="386.0" prefWidth="608.0">
         <columns>
            <TableColumn fx:id="columnId" prefWidth="37.0" text="ID" />
            <TableColumn fx:id="columnName" prefWidth="66.0" text="Название" />
            <TableColumn fx:id="columnStrength" prefWidth="68.0" text="Прочность" />
            <TableColumn fx:id="columnWeight" prefWidth="31.0" text="Вес" />
            <TableColumn fx:id="columnSize" prefWidth="50.0" text="Размер" />
            <TableColumn fx:id="columnThickness" prefWidth="59.0" text="Толщина" />
            <TableColumn fx:id="columnAppearance" prefWidth="86.0" text="Внешний вид" />
            <TableColumn fx:id="columnStandart" prefWidth="57.0" text="Образец" />
            <TableColumn fx:id="columnBatch" prefWidth="47.0" text="Партия" />
            <TableColumn fx:id="columnWorker" prefWidth="103.0" text="Работник" />
         </columns>
      </TableView>
      <Button fx:id="buttonAdd" layoutX="623.0" layoutY="14.0" mnemonicParsing="false" onAction="#Add_Pressed" prefHeight="61.0" prefWidth="124.0" text="Добавить" />
      <Button fx:id="buttonDelete" disable="true" layoutX="623.0" layoutY="161.0" mnemonicParsing="false" onAction="#Delete_Pressed" prefHeight="61.0" prefWidth="124.0" text="Удалить" />
      <Button fx:id="buttonEdit" disable="true" layoutX="623.0" layoutY="86.0" mnemonicParsing="false" onAction="#Edit_Pressed" prefHeight="61.0" prefWidth="124.0" text="Редактировать" />
      <Button fx:id="buttonBack" layoutX="623.0" layoutY="237.0" mnemonicParsing="false" onAction="#Back_Pressed" prefHeight="61.0" prefWidth="124.0" text="Назад" />
   </children>
</AnchorPane>
